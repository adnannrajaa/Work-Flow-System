
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title - Work Flow Application</title>
      <!-- plugins:css -->
    <link rel="stylesheet" href="~/DesignTemplate/src/assets/vendors/iconfonts/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/DesignTemplate/src/assets/vendors/iconfonts/ionicons/css/ionicons.css">

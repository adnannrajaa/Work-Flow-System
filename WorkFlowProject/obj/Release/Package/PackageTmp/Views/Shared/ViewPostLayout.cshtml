<!doctype html>
<html class="no-js h-100" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewBag.Title - Work Flow Application</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel='stylesheet' id='global-css-css'
              href='https://mdbootstrap.com/wp-content/plugins/mdb-forum/assets/css/global.css?ver=2.1.1' type='text/css'
              media='all' />
        <link rel='stylesheet' id='wysiwyg-css-css'
              href='https://mdbootstrap.com/wp-content/plugins/mdb-forum/assets/css/wysiwyg.css?ver=2.1.1' type='text/css'
              media='all' />
        <link rel='stylesheet' id='compiled.css-css'
              href='https://mdbootstrap.com/wp-content/themes/mdbootstrap4/css/compiled-4.8.9.min.css?ver=4.8.9'
              type='text/css' media='all' />

    <link href="https://use.fontawesome.com/releases/v5.0.6/css/all.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    <link rel="stylesheet" id="main-stylesheet" data-version="1.1.0" href="~/DesignTemplate/HTML/styles/shards-dashboards.1.1.0.min.css">
    <link rel="stylesheet" href="~/DesignTemplate/HTML/styles/extras.1.1.0.min.css" />

    <script async defer src="https://buttons.github.io/buttons.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/quill/1.3.6/quill.snow.css">
    <link rel="shortcut icon" href="~/DesignTemplate/src/assets/images/favicon.png" />

</head>
<body class="h-100">
    <div class="container-fluid">
        <div class="row">
            @Html.Partial("AsidePartial")
            <main class="main-content col-lg-10 col-md-9 col-sm-12 p-0 offset-lg-2 offset-md-3">
                @{

                    Html.RenderAction("GetUserDetailsAndNotifications");
                }
                <div class="main-content-container container-fluid px-4">
                    @RenderBody()
                </div>
                @Html.Partial("FooterPartial")
            </main>


        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.1/Chart.min.js"></script>
    <script src="https://unpkg.com/shards-ui@latest/dist/js/shards.min.js"></script>

    <script src="~/DesignTemplate/HTML/scripts/extras.1.1.0.min.js"></script>
    <script src="~/DesignTemplate/HTML/scripts/shards-dashboards.1.1.0.min.js"></script>

    <script type='text/javascript'
            src='https://mdbootstrap.com/wp-content/plugins/mdb-forum/assets/js/question.js?ver=2.1.1'></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/quill/1.3.6/quill.min.js"></script>
    <script src="~/DesignTemplate/HTML/scripts/app/app-blog-new-post.1.1.0.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script type="text/javascript" src="~/scripts/Scripts.js"></script>
</body>
</html>